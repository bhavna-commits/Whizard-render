<div id="previewSection" class="hidden">
	<div class="overflow-x-auto">
		<% if (headers && data) { %>
		<table class="min-w-full table-auto">
			<thead>
				<tr>
					<!-- Dynamically Render Headers -->
					<% headers.forEach((header) => { %>
					<th class="px-4 py-2 border"><%= header %></th>
					<% }); %>
				</tr>
			</thead>
			<tbody>
				<!-- Render Row Data -->
				<% data.forEach((row, rowIndex) => { %>
				<tr>
					<% headers.forEach((header) => { %>
					<td class="px-4 py-2 border <%= row[header] ? '' : 'bg-red-100' %>">
						<%= row[header] || '' %>
					</td>
					<% }); %>
				</tr>
				<% }); %>
			</tbody>
		</table>
		<% } %>
	</div>
	
	<div class="text-red-500 text-sm mt-4" id="errorMessage">
		
	</div>
	
	<div class="flex justify-end mt-6">
		<button id="reUpload" class="mr-4 bg-gray-200 hover:bg-gray-300 text-gray-700 font-semibold py-2 px-4 rounded">Re-upload</button>
		<button id="submitData" class="bg-blue-600 hover:bg-blue-700 text-white font-semibold py-2 px-4 rounded">Submit</button>
	</div>
</div>